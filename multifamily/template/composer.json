{
    "name": "febinqburst/${{ values.app_name | lower }}",
    "description": "${{ values.app_name | title }} - Beautiful PHP Web Application with Modern UI, API endpoints, Docker deployment and Jenkins CI/CD (Port 8086)",
    "type": "project",
    "version": "1.0.0",
    "keywords": [
        "php",
        "web",
        "api",
        "docker",
        "jenkins",
        "backstage"
    ],
    "authors": [
        {
            "name": "${{ values.user_name }}",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8086 index.php",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
} 